{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "VERCEL": "1",
    "SECRET_KEY": "feedmusic-sk",
    "ALGORITHM": "HS256",
    "ACCESS_TOKEN_EXPIRE_MINUTES": "60",
    "DEBUG": "False",
    "BACKEND_CORS_ORIGINS": "*"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}