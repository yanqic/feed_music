{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "VERCEL": "1",
    "SECRET_KEY": "your-secret-key-change-this-in-production",
    "ALGORITHM": "HS256",
    "ACCESS_TOKEN_EXPIRE_MINUTES": "60",
    "DEBUG": "False",
    "BACKEND_CORS_ORIGINS": "http://localhost:3000,http://localhost:3001,https://feed-music-frontend.vercel.app"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}